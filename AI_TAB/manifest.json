{
  "manifest_version": 3,
  "name": "AI 智能标签页分组",
  "version": "1.1",
  "description": "使用 AI 自动将打开的标签页进行分组。",
  "permissions": [
    "tabs",
    "tabGroups",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "AI 标签页分组设置",
    "default_popup": "popup.html"
  }
  
}
